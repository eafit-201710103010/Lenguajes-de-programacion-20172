inicio:
   readInt(a)
   readInt(b)
   readInt(c)
   readInt(d)
   uno:=1
   cero:=0

c1: if a<=b then goto nop
c2: if b<=c then goto nop
c3: if c<=d then goto nop
writeInt(uno)
goto fin

nop: writeInt(cero)
goto fin

fin:halt

equ cero M[0]
equ uno  M[1]
equ a    M[2]
equ b    M[3]
equ c    M[4]
equ d    M[5]